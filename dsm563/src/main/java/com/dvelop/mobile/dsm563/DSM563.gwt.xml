<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='dsm563'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <inherits name='com.google.gwt.user.theme.standard.Standard'/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.dvelop.mobile.dsm563.client.InputBugEntryPoint'/>
  
  	<inherits name="com.googlecode.mgwt.MGWT" />
	<set-configuration-property name="mgwt.css" value="pretty" /> 

	<inherits name="com.googlecode.gwtphonegap.PhoneGap" />
	<set-property name="user.agent" value="safari" />

	<!-- 
	<set-property name="mgwt.os" value="iphone,ipad,retina,android,android_tablet,desktop" />
	<set-property name="phonegap.env" value="no"/>
	-->	

	<add-linker name="xsiframe"/>

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
</module> 